{# Resources/views/layout/stylesheets.html.twig #}
{% set _webPath = "bundles/wbwsyntaxhighlighter" %}
{% set _version = getContainerParameter("wbw.syntaxhighlighter.asset.syntaxhighlighter").version %}
{% set _theme   = getContainerParameter("wbw_syntaxhighlighter.theme") %}
<!-- SyntaxHighlighter -->
<link type="text/css" rel="stylesheet" href="{{ asset(_webPath ~ "/syntaxhighlighter-" ~ _version ~ "/styles/shCore.css") }}">
<link type="text/css" rel="stylesheet" href="{{ asset(_webPath ~ "/syntaxhighlighter-" ~ _version ~ "/styles/shTheme" ~ _theme ~ ".css") }}">
