{
    "name": "webeweb/syntaxhighlighter-bundle",
    "type": "symfony-bundle",
    "description": "Integrate SyntaxHighlighter with Symfony 2",
    "keywords": ["webeweb", "bundle", "symfony", "syntaxhighlighter"],
    "homepage": "http://github.com/webeweb/syntaxhighlighter-bundle",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "http://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/framework-bundle": "^2.6|^3.0",
        "symfony/translation": "^2.6|^3.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/core-library": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*"
    },
    "support": {
        "issues": "http://github.com/webeweb/syntaxhighlighter-bundle/issues"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\SyntaxHighlighterBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\SyntaxHighlighterBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
